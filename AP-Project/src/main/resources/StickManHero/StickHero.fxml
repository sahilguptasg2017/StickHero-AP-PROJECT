<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="606.0" prefWidth="387.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StickManHero.StickHeroController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
         <ImageView fitHeight="635.0" fitWidth="791.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../background.png" />
            </image>
         </ImageView>
         <Button layoutX="334.0" layoutY="433.0" mnemonicParsing="false" onAction="#onStartButtonClick" prefHeight="30.0" prefWidth="54.0" text="Start">
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </Button>
         <Button layoutX="334.0" layoutY="490.0" mnemonicParsing="false" onAction="#showExitConfirmationDialog" prefHeight="30.0" prefWidth="54.0" text="Exit">
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </Button>
      </children>
   </AnchorPane>
</VBox>
